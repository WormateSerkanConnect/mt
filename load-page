<script>
  $(function() {
    let $wmTabs = $("#wm-tabs > ul > li a");
    $wmTabs.hide = function(){ $wmTabs.addClass('inactive').removeClass('active').each(function(){ $(this.getAttribute('href')).hide() });}
    $wmTabs.click(function(){
      $wmTabs.hide();
      $($(this).removeClass('inactive').addClass('active').attr('href')).show();
      return false;
    }); $wmTabs.hide();
    $("#wm-tabs li").removeClass("ui-corner-top").addClass("ui-corner-left");
  });
</script>
